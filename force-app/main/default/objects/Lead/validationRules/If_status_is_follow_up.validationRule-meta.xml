<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>If_status_is_follow_up</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL( Lead_status__c , &apos;Follow-up&apos;),ISBLANK(Follow_Up_Date__c))</errorConditionFormula>
    <errorDisplayField>Follow_Up_Date__c</errorDisplayField>
    <errorMessage>Fill Follow Up Date</errorMessage>
</ValidationRule>
